<div class="row">
  <div class="col s6 offset-s3">
     <% @users.each do |user| %>

          <div class="card">
            <div class="card-content">
              <div class="row">
                  <div class="col s2">
                    <%= image_tag current_user.avatar.url(:medium), class: "avatar-profile-avatar" %>
                  </div>
                  <div class="col s10">
                    <%= user.name %>
                  </div>
              </div>
            </div>
          </div>

     <% end %>
  </div>
</div>

